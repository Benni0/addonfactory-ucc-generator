{
    "env": {
        "browser": true,
        "amd": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-undef": 2
    },
    "globals": {
        "__webpack_public_path__": true,
        "__CONFIG_FROM_FILE__": true,
        "Splunk": true,
        "requirejs": true,
        "sprintf": true
    }
}
