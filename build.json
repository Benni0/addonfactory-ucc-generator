{
    "name": "Splunk_TA_crowdstrike",
    "version": "1.0.0",
    "friendly": {
        "name": "Splunk Add-on for Crowdsrtike"
    },
    "ponydocs": {
        "shortname": "Crowdsrtike"
    },
    "packagefiles": {
        "files": [
            {"dir": "package", "src": ["**/*", "!**/*.pyc", "!**/*.so"], "dest": "stage"}
        ],
        "configurations": [
            {"src": "package/metadata/default.meta", "dest": "stage/metadata/default.meta"}
        ]
    },
    "hooks": {
        "prebundle": ["shell:webpack", "shell:style"]
    },
    "extension": "spl"
}
