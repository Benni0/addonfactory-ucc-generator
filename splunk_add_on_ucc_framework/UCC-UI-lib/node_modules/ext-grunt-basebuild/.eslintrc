{
  "extends": "airbnb/legacy",
  "env": {
    "amd": true,
    "jasmine": true
  },
  "globals": {
    "Splunk": true
  },
  "plugins": [
    "backbone"
  ],
  "format": "stylish",
  "rules": {
    "camelcase": 0,
    "func-names": 0,
    "max-len": [
      2,
      150
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": [
          "Deferred",
          "Event"
        ]
      }
    ],
    "valid-jsdoc": 2,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "vars-on-top": 0,
    "backbone/collection-model": 2,
    "backbone/defaults-on-top": 2,
    "backbone/model-defaults": 2,
    "backbone/no-constructor": 2,
    "backbone/no-native-jquery": 0
  }
}
