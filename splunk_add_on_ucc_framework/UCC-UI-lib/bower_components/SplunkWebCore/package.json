{
  "name": "Splunk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "symlink-ui": "node scripts/symlink-ui.js",
    "config-ui": "node scripts/config-ui.js",
    "dev-setup": "npm run symlink-ui && npm run config-ui",
    "start": "node build_tools/build build_tools/profiles/pages_enterprise.config.js -dwr",
    "start:lite": "node build_tools/build build_tools/profiles/pages_lite.config.js -dwr",
    "build": "node build",
    "build:dev": "node build -d",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js --single-run",
    "test:watch": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "test:firefox": "node node_modules/karma/bin/karma start test/karma.conf.js --browsers Firefox",
    "test:chrome": "node node_modules/karma/bin/karma start test/karma.conf.js  --browsers Chrome",
    "build:splunkjs-independent": "node scripts/build-splunkjs-independent.js",
    "ci:lint": "npm run ci:lint:es+jsx && npm run ci:lint:js",
    "ci:lint:es+jsx": "eslint -f junit --ext .es,.jsx -o eslint.xml ./ ../cfg/bundles/corejs_test ../cfg/bundles/splunkjs_test || true",
    "ci:lint:js": "grunt jshintAndReport",
    "lint": "npm run lint:es+jsx && npm run lint:js",
    "lint:es+jsx": "eslint --ext .es,.jsx ./ ../cfg/bundles/corejs_test ../cfg/bundles/splunkjs_test || true",
    "lint:js": "grunt jshint",
    "doc": "jsdoc -c allJsDocConf.json && jsdoc -c publicJsDocConf.json"
  },
